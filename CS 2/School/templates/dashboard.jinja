<title> Dashboard </title>

<body>
    <h1> Dashboard </h1>
    <table>
        <tr>
            <th> First Name </th>
            <th>&nbsp&nbsp&nbsp</th>
            <th> Last Name </th>
            <th>&nbsp&nbsp&nbsp</th>
            <th> Password </th>
            <th>&nbsp&nbsp&nbsp</th>
            <th> Country of Orign </th>
        </tr>
        {% for s in students %}
            <tr>
                {% if request.session.login_user == s.firstname %}
                    <th> <a href = "{% url 'get_student' s.firstname %}"> {{s.firstname}} </a> (you) </th>
                {% else %}
                    <th> <a href = "{% url 'get_student' s.firstname %}"> {{s.firstname}} </a> </th>
                {% endif %}
                <th>&nbsp&nbsp&nbsp</th>
                <th> {{s.lastname}} </th>
                <th>&nbsp&nbsp&nbsp</th>
                <th> {{s.password}} </th>
                <th>&nbsp&nbsp&nbsp</th>
                <th> {{s.origin_country}} </th>
            </tr>
        {% endfor %}
    </table>
</body>

